    {
      "cell_type": "code",
      "source": [
        "fig, ax = plt.subplots()\n",
        "x = np.arange(len(stores))\n",
        "ax.bar(x, male, label='Male', color='blue')\n",
        "ax.bar(x, female, bottom=male, label='Female', color='green')\n",
        "\n",
        "ax.set_title('Customized Stacked Vertical Bar Chart')\n",
        "ax.set_xlabel('Stores')\n",
        "ax.set_ylabel('Population')\n",
        "ax.set_xticks(x, stores)\n",
        "ax.legend()\n",
        "\n",
        "plt.show()"
