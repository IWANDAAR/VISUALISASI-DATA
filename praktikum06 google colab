            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "width = 0.4\n",
        "x = np.arange(len(stores))\n",
        "\n",
        "# quarter 1\n",
        "plt.bar(x - width/2, q1_male, label='Q1 Male', color='lightblue', width=width)\n",
        "plt.bar(x - width/2, q1_female, bottom=q1_male, label='Q1 Female', color='pink', width=width)\n",
        "\n",
        "# quarter 2\n",
        "plt.bar(x + width/2, q2_male, label='Q2 Male', color='blue', width=width)\n",
        "plt.bar(x + width/2, q2_female, bottom=q2_male, label='Q2 Female', color='red', width=width)\n",
        "\n",
        "# quarter 3\n",
        "plt.bar(x + width/2, q3_male, label='Q3 Male', color='lightcoral', width=width)\n",
        "plt.bar(x + width/2, q3_female, bottom=q3_male, label='Q3 Female', color='yellow', width=width)\n",
        "\n",
        "# quarter 4\n",
        "plt.bar(x + width/2, q4_male, label='Q4 Male', color='black', width=width)\n",
        "plt.bar(x + width/2, q4_female, bottom=q4_male, label='Q4 Female', color='green', width=width)\n",
        "\n",
        "plt.title('Population by Gender and Store')\n",
        "plt.xlabel('Stores')\n",
        "plt.ylabel('Population')\n",
        "plt.xticks(x, stores) # Corrected to use plt.xticks to set both positions and labels\n",
        "plt.legend(bbox_to_anchor=(1.05, 1), loc='upper left')\n",
        "plt.show() # Added plt.show() to display the plot"
      ]
