      "source": [
        "x = np.arange(len(stores))\n",
        "plt.bar(x, male, label='Male', color='blue')\n",
        "plt.bar(x, female, bottom=male, label='Female', color='pink')\n",
        "\n",
        "plt.title('Population by Gender and Store')\n",
        "plt.xlabel('Stores')\n",
        "plt.ylabel('Population')\n",
        "plt.xticks(x, stores)\n",
        "plt.legend()\n",
        "\n",
